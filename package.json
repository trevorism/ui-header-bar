{
  "name": "@trevorism/ui-header-bar",
  "version": "3.4.0",
  "description": "The header bar component for Trevorism.com",
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "accept": "vitest run --environment jsdom",
    "dev": "vite",
    "preview": "vite preview --port 4173",
    "test": "vitest --environment jsdom",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "main": "./dist/ui-header-bar.umd.js",
  "module": "./dist/ui-header-bar.es.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "vite-plugin-css-injected-by-js": "^3.1.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1",
    "vue3-click-away": "^1.2.4",
    "vuestic-ui": "^1.6.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.1",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "vite": "^4.3.9",
    "vitest": "^0.31.1",
    "vue-cli-plugin-vuestic-ui": "~1.0.11",
    "vue3-cookies": "^1.0.6"
  },
  "peerDependencies": {
    "vue": "^3.2.38"
  },
  "license": "MIT",
  "repository": "trevorism/ui-header-bar"
}
