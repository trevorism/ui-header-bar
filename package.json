{
  "name": "@trevorism/ui-header-bar",
  "version": "3.5.0",
  "description": "The header bar component for Trevorism.com",
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "accept": "vitest run --environment jsdom",
    "dev": "vite",
    "preview": "vite preview --port 4173",
    "format": "prettier --write src/",
    "test": "vitest --environment jsdom",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "main": "./dist/ui-header-bar.umd.js",
  "module": "./dist/ui-header-bar.es.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5",
    "vue3-click-away": "^1.2.4",
    "vuestic-ui": "^1.8.7"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-cli-plugin-vuestic-ui": "~1.0.11",
    "vue3-cookies": "^1.0.6"
  },
  "peerDependencies": {
    "vue": "^3.2.38"
  },
  "license": "MIT",
  "repository": "trevorism/ui-header-bar"
}
